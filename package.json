{
  "name": "portfolio",
  "version": "1.0.0",
  "private": true,
  "description": "Minimal portfolio projects page with React, TypeScript, ESLint and Prettier.",
  "scripts": {
    "dev": "vite --host --port 4173 --strictPort",
    "start": "npm run dev",
    "build": "vite build",
    "preview": "npm run build && vite preview --host --port 4173 --strictPort",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "husky": "^9.0.0",
    "typescript": "^5.0.0",
    "vite": "^6.2.0",
    "vitest": "^2.1.8"
  }
}
